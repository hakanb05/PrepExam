            {
              "id": "A",
              "text": "Endoplasmic reticulum glycosyltransferase"
            },
            {
              "id": "B",
              "text": "Lysosomal α-glucosidase"
            },
            {
              "id": "C",
              "text": "Mitochondrial tRNA Leu"
            },
            {
              "id": "D",
              "text": "Nuclear proteasome activator"
            },
            {
              "id": "E",
              "text": "Peroxisomal catalase"
            }
